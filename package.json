{
  "name": "react-blog-poc",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "platform": {
    "linux": "linux/amd64",
    "macos": "linux/arm64/v8"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start:dev": "vite --port 3000",
    "start:build": "tsc && vite build",
    "start:server": "ts-node src/server/index.ts",
    "sync": "npx typesync && bun i",
    "preview": "vite preview",
    "commit": "./scripts/commit.sh",
    "test": "jest --verbose",
    "prepare": "husky install",
    "docker:build": "docker build -t react-blog-poc . --platform linux/amd64",
    "docker:run": "docker run -p 3000:3000 react-blog-poc",
    "docker:sync": "./scripts/docker-sync.sh",
    "docker:up": "./scripts/docker-up.sh",
    "start": "vite --host"
  },
  "dependencies": {
    "json-server": "^0.17.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^8.3.2",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-15": "^1.0.6",
    "@types/eslint": "^8.4.5",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/json-server": "^0.14.4",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.0.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-no-mixed-operators": "^1.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.14.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.54.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "typesync": "^0.9.2",
    "vite": "^2.5.10"
  }
}
