    "eslint-plugin-sonarjs": "^0.14.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
